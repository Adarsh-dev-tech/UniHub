{
  "name": "unihub",
  "version": "1.0.0",
  "description": "A centralized platform for college students to upload, share, and find course-specific academic resources",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd backend && npm run dev",
    "client": "cd frontend && npm start",
    "install-server": "cd backend && npm install",
    "install-client": "cd frontend && npm install",
    "install-all": "npm run install-server && npm run install-client",
    "build": "cd frontend && npm run build"
  },
  "keywords": ["education", "resources", "students", "academic", "sharing"],
  "author": "UniHub Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^7.6.0"
  }
}